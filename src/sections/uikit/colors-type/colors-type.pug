include ../../../components/uikit/colors-type/color-declaration/color-declaration
include ../../../components/uikit/colors-type/type/type

- var colors = [{colorName: "Dark Shade 100%", colorRGB: "#1F2041"}, {colorName: "Dark Shade 75%", colorRGB: "#1F2041"}, {colorName: "Dark Shade 50%", colorRGB: "#1F2041"}, {colorName: "Dark Shade 25%", colorRGB: "#1F2041"}, {colorName: "Dark Shade 5%", colorRGB: "#1F2041"}, {colorName: "Purple", colorRGB: "#BC9CFF"}, {colorName: "Green", colorRGB: "#6FCF97"}]

section.colors-type.none
    .container
        .colors-type__colors
            for i in colors
                +color-declaration({colorName: i.colorName, colorRGB: i.colorRGB})
        .colors-type__type
            +type